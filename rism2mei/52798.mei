	<music>
		<body>
			<mdiv>
				<score>
					<section n="1.1.1">
						<staff n="1">
							<layer>
								<tempo staff="1">Vorspiel Andante con moto (Tonbild aus dem Leben
									Diokletians.)</tempo>
								<dir staff="1"/>
							</layer>
						</staff>
					</section>
					<section n="1.10.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.9. Duett Andante</tempo>
								<dir staff="1">O Vater wird sind in enger Kerkerhaft</dir>
							</layer>
						</staff>
					</section>
					<section n="1.11.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.10. Lied der Soldaten des Sebastianus. Vivo</tempo>
								<dir staff="1">Auf ins Feld uns gehört die Welt</dir>
							</layer>
						</staff>
					</section>
					<section n="1.12.1">
						<staff n="1">
							<layer>
								<tempo staff="1">IV.Akt. No.11. Melodram. Allegro moderato</tempo>
								<dir staff="1"/>
							</layer>
						</staff>
					</section>
					<section n="1.13.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.12. Marsch und Chor Solenne</tempo>
								<dir staff="1"/>
							</layer>
						</staff>
					</section>
					<section n="1.13.2">
						<staff n="1">
							<layer>
								<tempo staff="1"/>
								<dir staff="1">Heil soweit die Meere den Erdball umkreisen</dir>
							</layer>
						</staff>
					</section>
					<section n="1.14.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.13a. Entreact und Chor. Maestoso</tempo>
								<dir staff="1"/>
							</layer>
						</staff>
					</section>
					<section n="1.14.2">
						<staff n="1">
							<layer>
								<tempo staff="1">Mit ruhiger nicht zu langsamer Bewegung</tempo>
								<dir staff="1">Suscepit Israel puerum suum</dir>
							</layer>
						</staff>
					</section>
					<section n="1.15.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.13b. Finale. Largo</tempo>
								<dir staff="1">O Herr Gott Israels sei du gepriesen</dir>
							</layer>
						</staff>
					</section>
					<section n="1.2.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No. 1 Introduction. (Siegesfeier in Rom.) Maestoso</tempo>
								<dir staff="1">Jo Triumph</dir>
							</layer>
						</staff>
					</section>
					<section n="1.3.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.2. Quartett Gemächlich</tempo>
								<dir staff="1">O Vater sei liebend gegrüßt</dir>
							</layer>
						</staff>
					</section>
					<section n="1.4.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.3. Recitativ und Arie. Andante</tempo>
								<dir staff="1">Was sinnest du und blickst ins Weite</dir>
							</layer>
						</staff>
					</section>
					<section n="1.4.2">
						<staff n="1">
							<layer>
								<tempo staff="1">Arie Andante grazioso</tempo>
								<dir staff="1">Sieh Freund mir ist es Luft</dir>
							</layer>
						</staff>
					</section>
					<section n="1.5.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.4. Finale. Moderato</tempo>
								<dir staff="1">Erklinget brausend Feierklänge</dir>
							</layer>
						</staff>
					</section>
					<section n="1.6.1">
						<staff n="1">
							<layer>
								<tempo staff="1">II.Akt. No.5. Entreact und Chor</tempo>
								<dir staff="1"/>
							</layer>
						</staff>
					</section>
					<section n="1.6.2">
						<staff n="1">
							<layer>
								<tempo staff="1"/>
								<dir staff="1">In paradisum deducant te angeli in tuo</dir>
							</layer>
						</staff>
					</section>
					<section n="1.7.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.6. Quintett und Chor Moderato</tempo>
								<dir staff="1">Gib heil'ger Vater mir den Segen</dir>
							</layer>
						</staff>
					</section>
					<section n="1.7.2">
						<staff n="1">
							<layer>
								<tempo staff="1"/>
								<dir staff="1">Aus unsres Elends Tiefen rufen wir</dir>
							</layer>
						</staff>
					</section>
					<section n="1.8.1">
						<staff n="1">
							<layer>
								<tempo staff="1">No.7. Finale. Risoluto</tempo>
								<dir staff="1">Gebt Raum hinweg</dir>
							</layer>
						</staff>
					</section>
					<section n="1.9.1">
						<staff n="1">
							<layer>
								<tempo staff="1">Akt III. No.8. Lied und Schwertertanz der Prätorianer. Allegro
									risoluto</tempo>
								<dir staff="1">An des Niles befruchtetem Strande</dir>
							</layer>
						</staff>
					</section>
